{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "indent": [
      true,
      "spaces"
    ],
    "jsx-boolean-value": false,
    "no-unused-variable": true
  }
}
